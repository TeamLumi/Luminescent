	_CALL('ev_another_out_end')
	_EVENT_END()
	_END()
ev_d29r0103_rei_lose:
	_TALK_OBJ_END()
	_CALL('ev_another_out_end')
	_EVENT_END()
	_END()
ev_d29r0103_legend_capture_ng:
	_TIME_WAIT(10, @SCWK_ANSWER)
	_OBJ_DEL('D29R0103_REI')
	_CALL_EFFECT(0, 'EF_F_CS_CHAP110_KIRA_YELLOW_END', -14, 1, 10)
	_TIME_WAIT(10, @SCWK_ANSWER)
	_TALKMSG('dp_scenario2%191-msg_d29r0103_sppoke1_02')
	_FLAG_SET(#FE_D29R0103_RETRY)
	_LAST_KEYWAIT()
	_TALK_CLOSE()
	_RELEASE_EFFECT(0)
	_TALK_OBJ_END()
	_CALL('ev_another_out_end')
	_EVENT_END()
	_END()
ev_d29r0103_legend_lose:

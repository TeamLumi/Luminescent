	_RIVAL_NAME(0)
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_c11_flintduo_6', 'See?\nI got a lot tougher, yeah?\rOK, I’m off to the Fronti...')
	_FLAG_RESET(#2882)
	_OBJ_ADD('C11_PALMER')
	_SE_PLAY('S_FI001')
	_TALKMSG('dp_scenario1%12-msg_t02_rival_01', 1)

ev_r206r0101_flag_change:
	_IFVAL_CALL(@WK_SCENE_C04R0101, 'GT', 1, 'ev_r206r0101_stoppers_flag')
	_IF_FLAGON_CALL(#FE_C04R0204_WIN, 'ev_r206r0101_stoppers_flag')

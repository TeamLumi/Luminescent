	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_r206r0101_girl2_02:
	_TALKMSG('dp_scenario2%20-msg_r206r0101_girl2_02')
	_LAST_KEYWAIT()
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()

	_JUMP('ev_pichu_event_valid')
ev_gsball_event:
	_TALK_OBJ_START()
	_TALKMSG('dp_scenario3%00-msgt05_gs_event_00')
	_TALK_KEYWAIT('dp_scenario3%00-msgt05_gs_event_01')
	_IFVAL_JUMP(@WK_SCENE_T05, 'LT', 2, 'ev_gsball_event_not_yet')
	_TALK_KEYWAIT('dp_scenario3%00-msgt05_gs_event_02')
	_YES_NO_WIN(@SCWK_ANSWER)
	_IFVAL_JUMP(@SCWK_ANSWER, 'EQ', 0, 'ev_gsball_event_yes')
	_TALK_CLOSE()
	_TALK_OBJ_END()
	_END()
ev_gsball_event_yes:
	_PLAYER_NAME(0)
	_TALKMSG('dp_scenario3%00-msgt05_gs_event_yes')
	_LDVAL(@SCWK_TEMP0, 1823); GS Ball
	_LDVAL(@SCWK_TEMP1, 1); Quantity 1
	_CHG_COMMON_SCR('ev_item_event_keywait')
	_TALK_CLOSE()
	_FLAG_SET(#2604); Kill 'ev_gsball_event' placedata
	_FLAG_RESET(#2605); Old man's work flag
	_SE_PLAY('S_FI001')
	_OBJ_ADD('T05_EVENT_OLDMAN')
	_OBJ_ANIME('T05_EVENT_OLDMAN', 'anm_gsball_oldman_walkto_player')
	_OBJ_ANIME('HERO', 'anm_gsball_oldman_walkto_player_hero')
	_OBJ_ANIME_WAIT()
	_TALK_KEYWAIT('dp_scenario3%00-msgt05_gs_event_03')
	_TALK_KEYWAIT('dp_scenario3%00-msgt05_gs_event_04')
	_TALK_KEYWAIT('dp_scenario3%00-msgt05_gs_event_05')
	_TALK_KEYWAIT('dp_scenario3%00-msgt05_gs_event_06')
	_TALK_KEYWAIT('dp_scenario3%00-msgt05_gs_event_07')
	_TALK_KEYWAIT('dp_scenario3%00-msgt05_gs_event_08')
	_TALK_CLOSE()
	_OBJ_ANIME('T05_EVENT_OLDMAN', 'anm_gsball_oldman_walkfrom_player')
	_OBJ_ANIME_WAIT()
	_FLAG_SET(#2605); Hide old man again
	_IF_FLAGOFF_CALL(#3109, 'ev_t05_ensure_celebi_flags_are_correct')
	_SE_PLAY('S_FI001')
	_OBJ_DEL('T05_EVENT_OLDMAN')
	_LDVAL(@WK_SCENE_D03R0101, 555)

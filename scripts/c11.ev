	_OBJ_ANIME('C11_RIVAL_2', 'anm_c11_action_19')
	_OBJ_ANIME('HERO', 'anm_c11_action_20')
	_OBJ_ANIME_WAIT()
	_OBJ_ANIME('C11_PALMER', 'anm_c11_action_21')
	_OBJ_ANIME_WAIT()
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_c11_flintduo_7', 'What’s this all about?!')
	_TALK_CLOSE()
	_RIVAL_NAME(0)
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_c11_flintduo_8', 'What the...\r...Daddy?!')
	_TALK_CLOSE()
	_OBJ_ANIME('C11_RIVAL_2', 'anm_c11_action_22')
	_OBJ_ANIME_WAIT()
	_RIVAL_NAME(0)
	_PLAYER_NAME(1)
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_c11_flintduo_9', 'Huh? Ah!\rIf it isn’t {0}!\nYou’ve grown all sturdy!\rAnd {1}, too! I haven’t\nseen you in a long time, either!')
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_c11_flintduo_10', '{1}, your mother told me\nyou’d gone off to the Battle Zone.\rSo I had to come running looking for\nyou.\rSo, are you two going to take the\nBattle Frontier challenge?\rThe Frontier had five facilities, but\nmost of them aren’t ready yet.\rEach one will have a unique set of\nrules, so they’re all different.\rOf course, you’ll have to be good to win\nat any of them.\rAnyway, I’m satisfied to see that\nyou’re both doing well.\rI’ll go back to my usual place in the\nBattle Tower.\rI’ll be looking forward to your\nchallenge.')
	_TALK_CLOSE()
	_OBJ_ANIME('C11_PALMER', 'anm_c11_action_23')
	_OBJ_ANIME_WAIT()
	_SE_PLAY('S_FI001')
	_OBJ_DEL('C11_PALMER')
	_FLAG_SET(#2882)
	_SE_WAIT('S_FI001')
	_RIVAL_NAME(0)
	_PLAYER_NAME(1)
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_c11_flintduo_11', 'My dad is cool, huh?\nI mean, when it gets right down to it.\fYou think so too, right?\rHe’s a Frontier Brain, after all.\nHe’s the Tower Tycoon!\rI want to be cool like my dad!\r{1}, that’s the one reason\nI decided to become a Trainer!')
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_c11_flintduo_12', 'Ooh, what am I going to do?\rShould I make the challenge now?\nOr do I train some more?')
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_c11_flintduo_13', 'OK! I’ve decided!\nI’m going to get tougher! By a lot!\rI finally made it here!\nThere’s no need for haste now.')
	_TALK_CLOSE()
	_OBJ_ANIME('C11_RIVAL_2', 'anm_c11_action_24')
	_OBJ_ANIME('HERO', 'anm_c11_action_25')
	_OBJ_ANIME_WAIT()
	_OBJ_DEL('C11_RIVAL_2')
	_FLAG_SET(#2881)
	_FLAG_RESET(#FV_C11_RIVAL)
	_OBJ_ANIME('C11_SEVEN2', 'anm_c11_action_26')
	_OBJ_ANIME('HERO', 'anm_c11_action_27')
	_OBJ_ANIME_WAIT()
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_c11_flintduo_14', 'Hey, hey.\nI just watched you two battle.\rYou’re something special, aren’t you?\nBeating an Elite Four like that.\rOh, right, howdy.\nThe name’s Buck.\rSay, why don’t you challenge our\nisland’s unofficial Gym challenge?\rSix routes on the island.\nOne trainer on each route.\rEach being an expert in a type that\nnobody in Sinnoh specialises in.\rSounds fun, right? You should do it.\nYou’ll toughen up for sure!\rAnd if you beat them all?\nWell... I’ll battle you!\rI’ll be training at Stark Mountain.\nBe seeing you!')
	_TALK_CLOSE()
	_OBJ_ANIME('C11_SEVEN2', 'anm_c11_action_28')
	_OBJ_ANIME('HERO', 'anm_c11_action_29')
	_OBJ_ANIME_WAIT()
	_OBJ_DEL('C11_SEVEN2')
